alias gtwork='cd ~/Desktop/workspace/'
alias gtproj='gtwork && cd projects'
alias gtmpos='gtproj && cd mpos'
alias gtdoc='gtmpos && cd doctors-web'

alias editvim='nvim ~/.config/nvim/init.vim'
alias editzsh='sudo nvim ~/.zshrc'
alias editalias='sudo nvim ~/.aliases'

alias git-config-coding='git config user.email codingchancez@gmail.com && git config user.name "Coding Chancez"'

alias git-bb-coding='ssh-add -D && ssh-add ~/.ssh/bitbucket-coding'
alias git-gh-aamiel='ssh-add -D && ssh-add ~/.ssh/github-aamiel'
alias git-cc-potech='ssh-add -D && ssh-add ~/.ssh/rsa_codecommit_potech'
alias git-cc-architech='ssh-add -D && ssh-add ~/.ssh/codecommit-architech'

alias rra='react-native run-android'
alias emulator='$ANDROID_HOME/emulator/emulator'
alias emulatorlist='emulator -list-avds'
alias stemulator='emulator @5.4_FWVGA_API_27'
alias emutablet='emulator @10.1_WXGA_Tablet_Oreo'
alias stsimulator='open /Applications/Xcode.app/Contents/Developer/Applications/Simulator.app'

alias k='kubectl'

alias docker-login-mpos='cat ~/Desktop/workspace/projects/mpos/keys/gcp/mpos-249109-3c75776515ec.json | docker login -u _json_key --password-stdin gcr.io'

alias profile-light='export ITERM_PROFILE=light'
alias profile-dark='export ITERM_PROFILE=dark'
